@model List<Category>

<h1>Category List</h1>
<table class=" table table-bordered table-dark"
    <thead>
        <tr>
        <th>
            Category
        </th>
        <th>
            Orders
        </th>
    </tr>
    <tbody>
            @foreach (var obj in Model.OrderBy(u => u.DisplayOrder))
            {
                <tr>
                     <td>@obj.Name</td>
                     <td>@obj.DisplayOrder</td>
                </tr>
            }
    </tbody>
    </thead>
    